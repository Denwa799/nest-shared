{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "denwa-nest-shared",
  "version": "1.0.3",
  "main": "./dist/src/main.js",
  "module": "./dist/src/main.js",
  "types": "./dist/main.d.ts",
  "author": "Denwa",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "swc src -d dist --delete-dir-on-start && tsc --emitDeclarationOnly --declaration --outDir dist",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" ",
    "prepare-push": "npm run lint && npm run format && npm run build",
    "release:minor": "standard-version minor",
    "release:patch": "standard-version patch"
  },
  "keywords": [],
  "license": "ISC",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-bee-ru/openapi-axios.git"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/main.d.ts"
      },
      "import": "./dist/src/main.js"
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.16",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "standard-version": "^9.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "@nestjs/swagger": "^11.0.3",
    "class-validator": "^0.14.1",
    "libphonenumber-js": "^1.11.20"
  }
}
